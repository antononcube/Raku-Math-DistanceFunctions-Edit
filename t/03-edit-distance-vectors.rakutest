use v6.d;

use lib <. lib>;
use Math::DistanceFunctions::Edit;

use Test;
plan *;

## 1
my @v1 = [1, 2, 32];
my @v2 = [1, 2, 55];
ok edit-distance(@v1, @v2);

## 2
is edit-distance(@v1, @v2), 1;

## 3
my @s1 = <a dre col er>;
my @s2 = <b dre bol rer>;
ok edit-distance(@s1, @s2);

## 4
is edit-distance(@s1, @s2), 3;

## 5
my @b1 = [True, False, True, False];
my @b2 = [True, True, True, False];

is edit-distance(@b1, @b2), 1;

done-testing;